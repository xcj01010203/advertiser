package com.xiaotu.common.util;

/**
 * 符号
 * @author xuchangjian
 */
public class SepratorUtil
{
	
	// 英文逗号
	public static final String SEP_COMMA_EN = ",";
	
	// 中文逗号
	public static final String SEP_COMMA_CN = "，";
	
	// 英文冒号
	public static final String SEP_COLON_EN = ":";
	
	// 中文冒号
	public static final String SEP_COLON_CN = "：";
	
	// 英文横杠
	public static final String SEP_CROSS_EN = "-";
	
	// 中文横杠
	public static final String SEP_CROSS_CN = "－";
	
	// 英文百分号
	public static final String SEP_PERCENT_EN = "%";
	
	// 英文加号"+"
	public static final String SEP_PLUS_EN = "+";
	
	// 分钟的标识
	public static final String SEP_MINUTE = "′";
	
	// 秒的标识
	public static final String SEP_SECONDS = "″";
	
	// 英文正斜杠
	public static final String SEP_SLASH_EN = "/";
	
	// 中文正斜杠
	public static final String SEP_SLASH_CN = "／";
	
	// 井号
	public static final String SEP_WELLNO_EN = "#";
	
	// 双引号
	public static final String SEP_DOUBLE_QUOTATION = "\"";
	
	// 单引号
	public static final String SEP_SINGLE_QUOTATION = "'";
	
	// 左括号
	public static final String SEP_LEFT_PARENTHESES = "(";
	
	// 右括号
	public static final String SEP_RIGHT_PARENTHESES = ")";
	
	// 左括号
	public static final String SEP_LEFT_PARENTHESES_CN = "（";
	
	// 右括号
	public static final String SEP_RIGHT_PARENTHESES_CN = "）";
	
	// 等号
	public static final String SEP_EQUALS = " = ";
	
	// 问号
	public static final String SEP_QUESTION = " ? ";
	
	// 空格
	public static final String SEP_SPACE_EN = " ";
	
	// </br>
	public static final String SEP_BR = "</br>";
	
	// ~
	public static final String SEP_MARK = "～";
	
	public static final String SEP_MARK_EN = "~";
	
	// _
	public static final String SEP_UNDERLINE = "_";
	
	public static final String SEP_POINT = ".";
	
	public static final String SEP_EQUAL = "=";
	
	public static final String SEP_PIPE = "|";
	
	public static final String SEP_QUOTATION = "\"";
	
	public static final String LINE_SEPRATOR = (String) java.security.AccessController
			.doPrivileged(new sun.security.action.GetPropertyAction(
					"line.separator"));
}
