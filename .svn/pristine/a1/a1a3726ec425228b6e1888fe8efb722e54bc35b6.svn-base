
/*分页插件的样式  start*/
.tcdPageCode {
    padding: 15px 20px;
    color: #ccc;
    text-align: center;
    margin-top: 40px;
    margin-bottom: 40px;
}
.tcdPageCode a {
    display: inline-block;
    color: #333;
    display: inline-block;
    height: 40px;
    line-height: 40px;
    padding: 0 15px;
    border: 1px solid #e9e9e9;
    margin: 0 4px;
    vertical-align: middle;
    text-decoration: none;
    transition: all 0.3s ease;
    background-color: #FFFFFF;
}
.tcdPageCode a:hover {
    text-decoration: none;
    border: 1px solid #29d9c2;
    background-color: #29d9c2;
    color: #fff;
}
.tcdPageCode span.current {
    display: inline-block;
    height: 40px;
    line-height:40px;
    padding: 0 15px;
    margin: 0 2px;
    color: #fff;
    background-color: #29d9c2;
    border: 1px solid #29d9c2;
    vertical-align: middle;
}
.tcdPageCode span.disabled {
    display: inline-block;
    height: 40px;
    line-height: 40px;
    padding: 0 15px;
    margin: 0 4px;
    color: #999;
    border: 1px solid #e9e9e9;
    vertical-align: middle;
    background-color: #FFFFFF;
}
/*分页插件的样式  end*/