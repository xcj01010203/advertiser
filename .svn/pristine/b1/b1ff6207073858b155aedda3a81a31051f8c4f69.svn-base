/*百分比*/
.percentage-box-con {
    margin: 0 8px;
}
.percentage-box-con .percentage-box {
    position: relative;
    display: inline-block;
    width: 130px;
    height: 12px;
    border-radius: 8px;
    background-color: #E7E7E7;
}
.percentage-box-con .percentage-box .percentage-box-color {
    position: absolute;
    height: 12px;
    border-radius: 8px;
    background-color: #29D9C2;
}
.percentage-box-con .percentage-box-num {
    display: inline-block;
    margin-left: 5px;
}