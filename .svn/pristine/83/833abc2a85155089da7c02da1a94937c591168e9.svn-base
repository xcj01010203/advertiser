    <div class="project-list-four">
        <!--剧本解析-->
        <div class="anal four-content center">
            <div class="four-left">
                剧本管理-剧本解析
            </div>
            <div class="four-middle"><span>请上传剧本</span><i class="icon iconfont down">&#xe604;</i><i class="icon iconfont top hide">&#xe608;</i>
            </div>
            <div class="four-right">
                <button class="upload-play upload-play-t">上传剧本</button>
            </div>
        </div>
    </div>

    <div class="project-list-five hide">
        <div class="five-content center">
            <div class="five-content-gather clearfix">
                <div class="gather-left">集次</div>
                <ul class="gather-right">
