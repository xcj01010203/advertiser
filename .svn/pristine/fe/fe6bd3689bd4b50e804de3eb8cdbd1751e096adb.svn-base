package com.xiaotu.advertiser.project.model;

import com.xiaotu.advertiser.user.model.UserModel;

/**
 * 书签信息
 * @author xuchangjian 2017年6月23日上午10:43:22
 */
public class BookMarksModel {

	private String id;
	private ProjectModel project;	//对应的项目
	private UserModel user;	//对应的用户
	private Integer type;	//书签类型：1-剧本书签
	private String value;	//书签值
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public ProjectModel getProject() {
		return project;
	}
	public void setProject(ProjectModel project) {
		this.project = project;
	}
	public UserModel getUser() {
		return user;
	}
	public void setUser(UserModel user) {
		this.user = user;
	}
	public Integer getType() {
		return type;
	}
	public void setType(Integer type) {
		this.type = type;
	}
	public String getValue() {
		return value;
	}
	public void setValue(String value) {
		this.value = value;
	}
}
